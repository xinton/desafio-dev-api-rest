generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["holders"]
}

model Holder {
  id        String   @id @default(uuid())
  name      String
  cpf       String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@schema("holders")
  @@map("holders")
}